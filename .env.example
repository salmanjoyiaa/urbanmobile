# Copy this file to .env.local and fill with real values. Never commit .env.local.
# For Vercel: set these in the project dashboard (Settings → Environment Variables).

# Supabase (required)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Upstash Redis (optional; needed for caching/rate limiting)
UPSTASH_REDIS_REST_URL=https://your-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Twilio WhatsApp (optional)
TWILIO_ACCOUNT_SID=ACxxxxxxxx
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886

# Twilio Content Template SIDs (WhatsApp approved templates; optional – defaults used if unset)
# TWILIO_TEMPLATE_VISIT_CONFIRMATION_CUSTOMER_SID=HX6e23b200047add8f129ffa4adcfc77cc
# TWILIO_TEMPLATE_VISIT_ASSIGNED_PROPERTY_AGENT_SID=HXa441955a0eadba3da289aa7deb88f8af
# TWILIO_TEMPLATE_VISIT_ASSIGNED_VISITING_AGENT_SID=HXc7193048915d62e308939c1033019656

# App (required for build)
NEXT_PUBLIC_SITE_URL=https://your-app.vercel.app
NEXT_PUBLIC_SITE_NAME=UrbanSaudi

# Admin WhatsApp – receives signup/visit notifications (optional)
ADMIN_WHATSAPP_NUMBER=+966500000000

# Resend email (optional)
RESEND_API_KEY=
RESEND_FROM_EMAIL=

# Sentry (optional)
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_DSN=
